{
  "name": "template-nuxt3",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.3",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "type:check": "vue-tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public",
    "preview": "nuxi preview",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxi prepare && npx simple-git-hooks"
  },
  "devDependencies": {
    "@css-render/vue3-ssr": "^0.15.12",
    "@eiog/eslint-config": "^0.0.2",
    "@eiog/ts-config": "^0.0.3",
    "@iconify-json/ri": "^1.1.12",
    "@iconify-json/twemoji": "^1.1.12",
    "@morev/vue-transitions": "^2.3.6",
    "@nuxt/content": "^2.8.2",
    "@nuxt/devtools": "^0.8.3",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "0.7.1",
    "@nuxt/ui": "^2.8.0",
    "@nuxthq/ui": "^2.7.0",
    "@nuxtjs/color-mode": "^3.3.0",
    "@nuxtjs/html-validator": "^1.5.2",
    "@nuxtjs/i18n": "8.0.0-rc.4",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/supabase": "^1.1.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.1.1",
    "@pinia/nuxt": "^0.4.11",
    "@types/crypto-js": "^4.1.2",
    "@types/express": "^4.17.17",
    "@types/nprogress": "^0.2.0",
    "@unocss/nuxt": "^0.55.7",
    "@vite-pwa/nuxt": "^0.1.0",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vitest/ui": "^0.34.3",
    "@vue/test-utils": "^2.4.1",
    "@vueuse/nuxt": "^10.4.1",
    "axios": "^1.5.0",
    "colord": "^2.9.3",
    "commitlint": "^17.7.1",
    "crypto-js": "^4.1.1",
    "cz-git": "^1.7.1",
    "czg": "^1.7.1",
    "eslint": "^8.48.0",
    "happy-dom": "^11.0.2",
    "jsdom": "^22.1.0",
    "less": "^4.2.0",
    "lint-staged": "^14.0.1",
    "naive-ui": "^2.34.4",
    "nprogress": "^0.2.0",
    "nuxt": "3.7.1",
    "nuxt-icon": "^0.5.0",
    "nuxt-icons": "^3.2.1",
    "nuxt-lazy-load": "^3.0.4",
    "nuxt-mongoose": "^0.0.9",
    "nuxt-security": "^0.14.4",
    "nuxt-typed-router": "^3.3.1",
    "nuxt-viewport": "^2.0.6",
    "nuxt-vitest": "^0.10.4",
    "postcss-preset-env": "^9.1.3",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.11.2",
    "typescript": "^5.2.2",
    "unocss": "^0.55.7",
    "unocss-preset-extra": "^0.5.2",
    "unocss-preset-scrollbar": "^0.2.1",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-inspector": "^0.4.0",
    "vfonts": "^0.0.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^0.34.3",
    "vue-tsc": "^1.8.10"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
}
