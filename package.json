{
  "name": "template-nuxt3",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.3",
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "type:check": "vue-tsc --noEmit",
    "start": "node .output/server/index.mjs",
    "generate": "nuxi generate",
    "start:generate": "npx serve .output/public",
    "preview": "nuxi preview",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxi prepare && npx simple-git-hooks"
  },
  "devDependencies": {
    "@bg-dev/nuxt-naiveui": "^1.7.2",
    "@css-render/vue3-ssr": "^0.15.12",
    "@eiog/eslint-config": "^0.0.2",
    "@eiog/ts-config": "^0.0.3",
    "@iconify-json/ri": "^1.1.18",
    "@iconify-json/twemoji": "^1.1.15",
    "@morev/vue-transitions": "^2.3.6",
    "@nuxt/content": "^2.9.0",
    "@nuxt/devtools": "^1.0.6",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/image": "1.1.0",
    "@nuxt/ui": "^2.11.1",
    "@nuxtjs/color-mode": "^3.3.2",
    "@nuxtjs/html-validator": "^1.6.0",
    "@nuxtjs/i18n": "8.0.0-rc.11",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/supabase": "^1.1.4",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^4.17.21",
    "@types/nprogress": "^0.2.3",
    "@unocss/nuxt": "^0.58.0",
    "@unscatty/unocss-preset-daisy": "^1.0.0",
    "@vite-pwa/nuxt": "^0.3.6",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vitest/ui": "^1.1.0",
    "@vue/test-utils": "^2.4.3",
    "@vueuse/nuxt": "^10.7.0",
    "axios": "^1.6.2",
    "colord": "^2.9.3",
    "commitlint": "^18.4.3",
    "crypto-js": "^4.2.0",
    "cz-git": "^1.8.0",
    "czg": "^1.8.0",
    "eslint": "^8.56.0",
    "happy-dom": "^12.10.3",
    "jsdom": "^23.0.1",
    "less": "^4.2.0",
    "lint-staged": "^15.2.0",
    "naive-ui": "^2.36.0",
    "nprogress": "^0.2.0",
    "nuxt": "3.8.2",
    "nuxt-icon": "^0.6.7",
    "nuxt-icons": "^3.2.1",
    "nuxt-lazy-load": "^3.0.4",
    "nuxt-mongoose": "^1.0.3",
    "nuxt-security": "1.0.0",
    "nuxt-typed-router": "^3.5.0",
    "nuxt-viewport": "^2.0.6",
    "nuxt-vitest": "^0.11.5",
    "postcss-preset-env": "^9.3.0",
    "simple-git-hooks": "^2.9.0",
    "taze": "^0.13.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "unocss-preset-extra": "^0.5.3",
    "unocss-preset-scrollbar": "^0.3.0",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-icons": "^0.18.1",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-inspector": "^1.0.2",
    "vfonts": "^0.0.3",
    "vite-plugin-svg-icons": "^2.0.1",
    "vitest": "^1.1.0",
    "vue-tsc": "^1.8.25"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
}
