{
  "name": "nuxt3-starter",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@8.6.3",
  "scripts": {
    "dev": "nuxi dev --host",
    "dev:pwa": "VITE_PLUGIN_PWA=true nuxi dev",
    "build": "nuxi build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update-deps": "taze -w && pnpm i",
    "type:check": "vue-tsc --noEmit",
    "generate": "nuxi generate",
    "prepare": "nuxi prepare",
    "start": "node .output/server/index.mjs",
    "start:generate": "npx serve .output/public",
    "preview": "nuxi preview",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxi prepare && npx simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.0",
    "@better-scroll/core": "^2.5.1",
    "@bg-dev/nuxt-naiveui": "^1.13.2",
    "@eiog/ts-config": "^0.0.3",
    "@iconify-json/ri": "^1.1.20",
    "@iconify-json/twemoji": "^1.1.15",
    "@morev/vue-transitions": "^3.0.2",
    "@nuxt/content": "^2.12.1",
    "@nuxt/devtools": "^1.3.1",
    "@nuxt/eslint": "^0.3.12",
    "@nuxt/eslint-config": "^0.3.12",
    "@nuxt/image": "1.7.0",
    "@nuxt/ui": "^2.16.0",
    "@nuxtjs/color-mode": "^3.4.1",
    "@nuxtjs/html-validator": "^1.8.1",
    "@nuxtjs/i18n": "8.3.1",
    "@nuxtjs/robots": "^3.0.0",
    "@nuxtjs/supabase": "^1.2.2",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@plugin-web-update-notification/vite": "^1.7.1",
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/nprogress": "^0.2.3",
    "@unocss/eslint-plugin": "^0.60.2",
    "@unocss/nuxt": "^0.60.2",
    "@unscatty/unocss-preset-daisy": "^1.0.0",
    "@vite-pwa/nuxt": "^0.7.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/ui": "^1.6.0",
    "@vue-hooks-plus/resolvers": "^1.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/motion": "^2.1.0",
    "@vueuse/nuxt": "^10.9.0",
    "axios": "^1.6.8",
    "colord": "^2.9.3",
    "commitlint": "^19.3.0",
    "crypto-js": "^4.2.0",
    "cz-git": "^1.9.1",
    "czg": "^1.9.1",
    "echarts": "^5.5.0",
    "eslint": "^9.2.0",
    "eslint-plugin-format": "^0.1.1",
    "happy-dom": "^14.11.0",
    "jsdom": "^24.0.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "naive-ui": "^2.38.2",
    "notivue": "^2.4.4",
    "nprogress": "^0.2.0",
    "nuxt": "3.11.2",
    "nuxt-icon": "^0.6.10",
    "nuxt-icons": "^3.2.1",
    "nuxt-lazy-load": "^3.0.4",
    "nuxt-mongoose": "^1.0.6",
    "nuxt-security": "2.0.0-beta.5",
    "nuxt-typed-router": "^3.6.3",
    "nuxt-viewport": "^2.1.5",
    "nuxt-vitest": "^0.11.5",
    "postcss-preset-env": "^9.5.13",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.13.8",
    "typed.js": "^2.1.0",
    "typescript": "^5.4.5",
    "unocss": "^0.60.2",
    "unocss-preset-extra": "^0.5.3",
    "unocss-preset-scrollbar": "^0.3.1",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-icons": "^0.19.0",
    "unplugin-info": "^1.1.0",
    "unplugin-turbo-console": "^1.8.6",
    "unplugin-vue-components": "^0.27.0",
    "unplugin-vue-cssvars": "^2.0.0",
    "vfonts": "^0.0.3",
    "vite-plugin-debug": "^1.0.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-version-mark": "^0.0.13",
    "vite-plugin-webfont-dl": "^3.9.4",
    "vitest": "^1.6.0",
    "vue-hooks-plus": "^2.0.3",
    "vue-tsc": "^2.0.17"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
}
